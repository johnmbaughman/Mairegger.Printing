{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/Mairegger.Printing/**.csproj"
          ],
          "exclude": [
            "**/obj/**",
            "**/bin/**",
            "_site/**"
          ]
        }
      ],
      "dest": "docs/api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ],
        "src":"docs"
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ],
        "src":"docs"
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ],
        "src":"docs"
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "src":"docs"
      }
    ],
    "dest": "_site",
    "globalMetadata":
	  {
      "_appTitle":"Mairegger.Printing",
	    "_gitContribute":
			{
			  "repo": "https://github.com/xxMUROxx/Mairegger.Printing",
			  "branch": "master",
			  "apiSpecFolder": "docs"
			}
	  },
    "template": [
      "default",
	    "statictoc"
    ],
    "postProcessors": [],
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false
  }
}
